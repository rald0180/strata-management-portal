{
  "rewrites": [
    {
      "source": "/old-home",
      "destination": "/index.html"
    }
  ],
  "regions": ["syd1"],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}